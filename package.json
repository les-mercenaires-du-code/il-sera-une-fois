{
  "name": "rssr",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "tmp": "babel-node src/server/devServer.js",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development npm run watch",
    "watch": "nodemon \"src/server\" --exec babel-node --watch \"./src/server\"",
    "prebuild": "rimraf build && mkdir -p build && cp -Rp src/public build",
    "build": "cross-env NODE_ENV=production npm-run-all build:server build:client build:server:client",
    "build:client": "webpack --config ./webpack.browser.js",
    "build:server:client": "webpack --config ./webpack.server.prod.js",
    "build:server": "babel ./src/server -d build/server",
    "prod": "cross-env NODE_ENV=production node build/server",
    "serve": "npm run build && npm run prod",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@emotion/babel-preset-css-prop": "^10.0.23",
    "@loadable/babel-plugin": "^5.10.3",
    "@loadable/webpack-plugin": "^5.7.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.15.1",
    "file-loader": "^4.2.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "^1.4.1",
    "sass-loader": "^7.0.3",
    "sass-resources-loader": "^2.0.1",
    "stats-webpack-plugin": "^0.7.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  },
